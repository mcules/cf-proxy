{
  "name": "cf-proxy",
  "version": "1.2.0",
  "engines": {
    "npm": "10.8.2",
    "node": "^20.18"
  },
  "bin": {
    "cf-destination-proxy": "./bin/cf-destination-proxy.js",
    "cf-ssh-proxy": "./bin/cf-ssh-proxy.js"
  },
  "devDependencies": {
    "cross-var": "^1.1.0",
    "mbt": "^1.2.7"
  },
  "dependencies": {
    "@sap/xsenv": "^3.1.0",
    "@sap/xssec": "3.6.0",
    "axios": "1.7.8",
    "commander": "^13.1.0",
    "dotenv": "^8.2.0",
    "http-proxy": "^1.18.1",
    "inquirer": "^8.0.0"
  },
  "scripts": {
    "build": "mbt build --source=./remote --target=../mta_archives",
    "dev:deploy": "npm run build && cross-var cf deploy mta_archives/cf-destination-proxy_$npm_package_version.mtar --delete-services"
  }
}
